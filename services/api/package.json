{
    "name": "onscoutline-api",
    "packageManager": "yarn@3.1.1",
    "engines": {
        "node": "14.17.5"
    },
    "scripts": {
        "build": "yarn tsc --build ",
        "cli": "yarn ts-node ./cli",
        "start:dev": "yarn nodemon ./src/index.ts",
        "start:prod": "yarn node ./dist/index.js",
        "test": "yarn test:db:init && yarn typeorm:test migration:run && yarn test:quick",
        "test:quick": "jest --colors --runInBand --coverage --forceExit",
        "test:db:init": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/testing/create-database.ts",
        "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/typeorm/cli.ts",
        "typeorm:test": "NODE_ENV=test yarn typeorm",
        "scraper:competitions": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/scraper/competitions.ts",
        "scraper:clubsUrls": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/scraper/clubs-urls.ts",
        "scraper:clubs": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/scraper/clubs.ts",
        "scraper:players": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/scraper/players.ts",
        "scraper:matchesUrls": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/scraper/matches-urls.ts",
        "scraper:matches": "node -r tsconfig-paths/register -r ts-node/register ./src/scripts/scraper/matches.ts",
        "code:check": "yarn prettier -l ./**/*.ts && yarn tsc --noemit"
    },
    "devDependencies": {
        "@types/eslint": "^8",
        "@types/express": "^4",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.18",
        "@types/prettier": "^2",
        "@types/yargs": "^17",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.1.4",
        "eslint-plugin-no-only-tests": "^2.6.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "tsconfig-paths": "^3.12.0"
    },
    "dependencies": {
        "apollo-server-express": "^3.6.3",
        "axios": "^0.26.0",
        "cockatiel": "^2.0.2",
        "envalid": "^7.2.2",
        "express": "^4.17.2",
        "helmet": "^5.0.2",
        "mysql2": "^2.3.3",
        "node-html-parser": "^5.2.0",
        "puppeteer": "^13.4.1",
        "typeorm": "^0.2.43",
        "typescript": "^4.5.5",
        "yargs": "^17.3.1"
    }
}
